{
  "lambda-basic-execution": {
    "policy_content": {
      "Statement": [
        {
          "Action": [
            "logs:CreateLogGroup",
            "logs:CreateLogStream",
            "logs:PutLogEvents",
            "dynamodb:GetItem",
            "dynamodb:Query",
            "dynamodb:PutItem",
            "dynamodb:BatchGetItem",
            "dynamodb:BatchWriteItem",
            "dynamodb:DeleteItem",
            "dynamodb:DescribeTable",
            "dynamodb:CreateTable",
            "dynamodb:UpdateItem",
            "dynamodb:Scan",
            "dynamodb:DeleteTable",
            "dynamodb:DescribeStream",
            "dynamodb:GetRecords",
            "dynamodb:GetShardIterator",
            "dynamodb:ListStreams",
            "ssm:PutParameter",
            "ssm:GetParameter",
            "ssm:GetParameters",
            "kms:Decrypt",
            "kms:Encrypt",
            "kms:GenerateDataKey",
            "kms:ReEncrypt*",
            "s3:GetBucketNotificationConfiguration",
            "s3:PutBucketNotificationConfiguration",
            "s3:GetObject",
            "s3:PutObject"
          ],
          "Effect": "Allow",
          "Resource": "*"
        }
      ],
      "Version": "2012-10-17"
    },
    "resource_type": "iam_policy"
  },
  "audit_producer-role": {
    "predefined_policies": [
      "AWSLambdaDynamoDBExecutionRole"
    ],
    "principal_service": "lambda",
    "custom_policies": [
      "lambda-basic-execution"
    ],
    "resource_type": "iam_role"
  },
  "${config_table}": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "id",
    "hash_key_type": "S",
    "read_capacity": 10,
    "write_capacity": 10,
    "global_indexes": [],
    "autoscaling": []
  },
  "${target_table}": {
    "resource_type": "dynamodb_table",
    "hash_key_name": "id",
    "hash_key_type": "S",
    "read_capacity": 10,
    "write_capacity": 10,
    "global_indexes": [],
    "autoscaling": []
  }
}